<h1>Books</h1>

<% @books.each do |book| %>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
</table>



  <p>Title</p>
  <span><%= book.title %></span>

  <p>Body</p>
  <span><%= book.body %></span>

<% end %>


<h2>New Book</h2>
<%= form_with model: @book do |f| %>
<h4>title</h4>
<%= f.text_field :title %>

<h4>body</h4>
<%= f. text_area :body %>

<%= f.submit :'Create Books' %>

<% end %>